<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="sub_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#247</item>
    </style>
    <style name="developer_item">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center|left</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="about_name_dev">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#456</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="about_email_dev">
        <item name="android:autoLink">email</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left|center_vertical</item>
        <item name="android:textSize">17dp</item>
        <item name="android:textColor">#777</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>
</resources>